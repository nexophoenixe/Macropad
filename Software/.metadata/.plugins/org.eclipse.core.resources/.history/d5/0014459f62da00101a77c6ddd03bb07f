#include "macropad.h"

uint16_t PORT_shifting(){
	uint16_t PORTA = GPIOA->IDR;// & 0b0000000011000000;
	uint16_t PORTB = GPIOB->IDR;// & 0b0011011100111110;

	uint16_t PORT = PORTB;
	PORT = PORT & 	0b1111111100111111

			| PORTB;
	return PORT;
}



